import java.util.Random;
import java.util.Scanner;

public class GuessTheNumber
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Random r = new  Random();
		int numtoguess = r.nextInt(10)+1;
		System.out.println("Guess  your number and check if your lucky");
		int userguess = sc.nextInt();
		
		if (userguess == numtoguess) {
			System.out.println("Congratulations! ðŸ˜Š You guesssed it right!");
			
		}
		else {
			System.out.println("OOoops ðŸ˜” you are wrong ...better luck next time and the num was : "+ numtoguess);
		}
		
		
	}
}
